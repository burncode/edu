<swiper indicator-dots="true" autoplay="true" interval="{{interval}}" duration="{{duration}}" indicator-active-color="#1aad19">
  <block wx:for="{{imgUrls}}" wx:key="*this">
    <swiper-item id="swiper-item">
      <image src="{{item}}" class="slide-image" />
    </swiper-item>
  </block>
</swiper>
