<view>
  <swiper indicator-dots="true" autoplay="true" interval="{{interval}}" duration="{{duration}}" indicator-active-color="#1aad19">
    <block wx:for="{{imgUrls}}" wx:key="{{index}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image" style="width:100%"/>
      </swiper-item>
    </block>
  </swiper>
  
</view>
